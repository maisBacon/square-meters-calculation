{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "airbnb-base","prettier"
    ],
    "plugins":["prettier"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
        "indent": [
            "error",
            2
          ],
          "linebreak-style": [
            "error",
            "unix"
          ],
          "quotes": [
            "error",
            "single"
          ],
          "semi": [
            "error",
            "always"
          ],
          "no-console":"off",
          "prettier/prettier": "error",
          "class-methods-use-this":"off",
          "no-return-await":"off"
    }
}
